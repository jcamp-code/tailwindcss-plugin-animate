{
  "name": "@jcamp/tailwindcss-plugin-animate",
  "version": "0.1.0",
  "description": "An updated animation plugin for Tailwind CSS v3",
  "main": "dist/index.js",
  "packageManager": "^pnpm@8.1.0",
  "scripts": {
    "dev": "tsc --watch & cd ./demo && npm run-script dev",
    "build": "tsc",
    "release": "np --no-2fa",
    "test": "tsc && c8 jest"
  },
  "license": "MIT",
  "author": "John Campion (https://github.com/JohnCampionJr/)",
  "repository": "https://github.com/jcamp-code/tailwindcss-plugin-animate",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.192",
    "c8": "^7.13.0",
    "jest": "^29.5.0",
    "np": "^7.7.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.3.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ]
  },
  "c8": {
    "exclude": [
      "jest/customMatchers.js",
      "**/*.test.js"
    ]
  },
  "keywords": [
    "tailwindcss",
    "animate",
    "animation"
  ],
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
